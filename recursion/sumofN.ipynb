    "# print the sum of n number using reacurtion \n",
    "\n",
    "def sumN(n):\n",
    "    if n==0 or n==1:\n",
    "        return 1\n",
    "    return n + sumN(n-1)\n",
    "\n",
    "sumN(5)"
   ]
