    "t=int(input())\n",
    "while (t>0):\n",
    "  n=int(input())\n",
    "  arr=map(int,input().split())\n",
    "  total=(n+1)*(n+2)//2\n",
    "  sub=sum(arr)\n",
    "  print(total-sub)\n",
    "  t=t-1"
