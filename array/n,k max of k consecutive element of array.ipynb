    "def kele(arr,k):\n",
    "    max=-1\n",
    "    sum=0\n",
    "    n=len(arr)\n",
    "    for i in range(0,n-k+1):\n",
    "        sum=0\n",
    "        for j in range(i,i+k):\n",
    "            sum+=arr[j]\n",
    "        if (max<sum):\n",
    "            max=sum\n",
    "    return sum\n",
    "\n",
    "arr=[1,2,3,4,5]\n",
    "kele(arr,3)\n"
   ]
